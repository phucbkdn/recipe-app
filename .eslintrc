{
  "extends": [
    "prettier",
    "airbnb"
  ],
  "parser": "babel-eslint",
  "rules": {
    "arrow-parens": 0,
    "arrow-body-style": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-dynamic-require": 0,
    "no-underscore-dangle": 0,
    "no-useless-escape": 0,
    "no-throw-literal": 0,
    "no-plusplus": 0,
    "no-param-reassign": 0,
    "global-require": 0
  },
  "plugins": [
    "prettier"
  ],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  }
}
